@using HSMServer.Model.UserTreeShallowCopy

@model List<BaseShallowModel>


<ul>
    @foreach (var node in Model)
    {
        @if (node is NodeShallowModel product)
        {
            @await Html.PartialAsync("~/Views/Tree/_TreeNode.cshtml", product)
        }
        else if (node is FolderShallowModel folder)
        {
            @await Html.PartialAsync("~/Views/Tree/_TreeFolder.cshtml", folder)
        }
    }
</ul>